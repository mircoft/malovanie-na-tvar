.fairy-container {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    pointer-events: none;
    z-index: 1;
    overflow: hidden;
}

.decorative-elements {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    pointer-events: none;
    z-index: 0;
    overflow: hidden;
}

.decorative-side {
    display: none; /* Hide decorative sides as fairies will fly across the whole page */
}

.decorative-stars {
    position: absolute;
    width: 100%;
    height: 100%;
    background-image: 
        radial-gradient(circle at 10% 20%, rgba(255, 224, 102, 0.3) 0%, transparent 2px),
        radial-gradient(circle at 20% 40%, rgba(181, 224, 255, 0.3) 0%, transparent 2px),
        radial-gradient(circle at 30% 60%, rgba(255, 214, 224, 0.3) 0%, transparent 2px),
        radial-gradient(circle at 40% 80%, rgba(224, 195, 252, 0.3) 0%, transparent 2px),
        radial-gradient(circle at 90% 30%, rgba(255, 224, 102, 0.3) 0%, transparent 2px),
        radial-gradient(circle at 80% 50%, rgba(181, 224, 255, 0.3) 0%, transparent 2px),
        radial-gradient(circle at 70% 70%, rgba(255, 214, 224, 0.3) 0%, transparent 2px),
        radial-gradient(circle at 60% 90%, rgba(224, 195, 252, 0.3) 0%, transparent 2px);
    animation: twinkle 8s ease-in-out infinite;
    opacity: 0.3;
}

.decorative-dots {
    position: absolute;
    width: 100%;
    height: 100%;
    background-image: 
        radial-gradient(circle at 15% 25%, rgba(255, 224, 102, 0.2) 0%, transparent 3px),
        radial-gradient(circle at 25% 45%, rgba(181, 224, 255, 0.2) 0%, transparent 3px),
        radial-gradient(circle at 35% 65%, rgba(255, 214, 224, 0.2) 0%, transparent 3px),
        radial-gradient(circle at 45% 85%, rgba(224, 195, 252, 0.2) 0%, transparent 3px),
        radial-gradient(circle at 85% 35%, rgba(255, 224, 102, 0.2) 0%, transparent 3px),
        radial-gradient(circle at 75% 55%, rgba(181, 224, 255, 0.2) 0%, transparent 3px),
        radial-gradient(circle at 65% 75%, rgba(255, 214, 224, 0.2) 0%, transparent 3px),
        radial-gradient(circle at 55% 95%, rgba(224, 195, 252, 0.2) 0%, transparent 3px);
    animation: float 12s ease-in-out infinite;
    opacity: 0.2;
}

@keyframes twinkle {
    0%, 100% { opacity: 0.1; }
    50% { opacity: 0.4; }
}

@keyframes float {
    0%, 100% { transform: translateY(0); }
    50% { transform: translateY(-10px); }
}

.fairy-animation {
    position: absolute;
    width: 150px;
    height: 150px;
    opacity: 0.7;
    filter: drop-shadow(0 0 15px rgba(255, 165, 0, 0.4));
    z-index: 1;
    animation: fairyFly1 60s infinite ease-in-out;
}

.fairy-animation.secondary {
    width: 130px;
    height: 130px;
    opacity: 0.6;
    filter: drop-shadow(0 0 12px rgba(147, 112, 219, 0.4));
    animation: fairyFly2 70s infinite ease-in-out reverse;
}

@keyframes fairyFly1 {
    0% { transform: translate(10vw, 25vh) rotate(0deg); }
    5% { transform: translate(15vw, 30vh) rotate(5deg); }
    10% { transform: translate(25vw, 15vh) rotate(-8deg); }
    15% { transform: translate(35vw, 40vh) rotate(10deg); }
    20% { transform: translate(50vw, 20vh) rotate(-5deg); }
    25% { transform: translate(65vw, 55vh) rotate(8deg); }
    30% { transform: translate(80vw, 30vh) rotate(-10deg); }
    35% { transform: translate(90vw, 60vh) rotate(5deg); }
    40% { transform: translate(75vw, 10vh) rotate(-12deg); }
    45% { transform: translate(60vw, 45vh) rotate(8deg); }
    50% { transform: translate(50vw, 70vh) rotate(-5deg); }
    55% { transform: translate(40vw, 35vh) rotate(10deg); }
    60% { transform: translate(25vw, 65vh) rotate(-8deg); }
    65% { transform: translate(5vw, 40vh) rotate(5deg); }
    70% { transform: translate(30vw, 15vh) rotate(-10deg); }
    75% { transform: translate(55vw, 50vh) rotate(8deg); }
    80% { transform: translate(70vw, 25vh) rotate(-5deg); }
    85% { transform: translate(90vw, 75vh) rotate(10deg); }
    90% { transform: translate(65vw, 5vh) rotate(-12deg); }
    95% { transform: translate(40vw, 30vh) rotate(8deg); }
    100% { transform: translate(10vw, 25vh) rotate(0deg); }
}

@keyframes fairyFly2 {
    0% { transform: translate(90vw, 75vh) rotate(0deg); }
    5% { transform: translate(85vw, 70vh) rotate(-5deg); }
    10% { transform: translate(75vw, 85vh) rotate(8deg); }
    15% { transform: translate(65vw, 60vh) rotate(-10deg); }
    20% { transform: translate(50vw, 80vh) rotate(5deg); }
    25% { transform: translate(35vw, 45vh) rotate(-8deg); }
    30% { transform: translate(20vw, 70vh) rotate(10deg); }
    35% { transform: translate(10vw, 40vh) rotate(-5deg); }
    40% { transform: translate(25vw, 90vh) rotate(8deg); }
    45% { transform: translate(40vw, 55vh) rotate(-10deg); }
    50% { transform: translate(50vw, 30vh) rotate(5deg); }
    55% { transform: translate(60vw, 65vh) rotate(-8deg); }
    60% { transform: translate(75vw, 40vh) rotate(10deg); }
    65% { transform: translate(95vw, 60vh) rotate(-5deg); }
    70% { transform: translate(70vw, 85vh) rotate(8deg); }
    75% { transform: translate(45vw, 50vh) rotate(-10deg); }
    80% { transform: translate(30vw, 75vh) rotate(5deg); }
    85% { transform: translate(10vw, 25vh) rotate(-8deg); }
    90% { transform: translate(5vw, 50vh) rotate(10deg); }
    95% { transform: translate(30vw, 10vh) rotate(-5deg); }
    100% { transform: translate(90vw, 75vh) rotate(0deg); }
}

