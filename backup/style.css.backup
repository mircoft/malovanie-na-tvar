    box-shadow: var(--apple-active-shadow);
    transition-duration: 0.1s;
}

.gallery img {
    border: var(--ios-border);
    box-shadow: var(--ios-shadow);
    transition: var(--transition);
    position: relative;
    overflow: hidden;
}

.gallery img::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(45deg, transparent, rgba(255, 255, 255, 0.3), transparent);
    transform: translateX(-100%);
    transition: transform 0.8s ease;
}

.gallery img:hover::before {
    transform: translateX(150%);
}

.gallery img:hover {
    transform: var(--apple-hover-scale);
    box-shadow: var(--apple-hover-shadow);
}

.gallery img:active {
    transform: var(--apple-active-scale);
    box-shadow: var(--apple-active-shadow);
    transition-duration: 0.1s;
}

.instagram-media {
    margin: 0 !important;
    width: 100% !important;
    max-width: 320px !important;
    min-width: unset !important;
    transition: var(--transition) !important;
    aspect-ratio: 9/16 !important;
    overflow: hidden;
    background: var(--glass-bg) !important;
    backdrop-filter: var(--blur) !important;
    -webkit-backdrop-filter: var(--blur) !important;
    border-radius: var(--radius-lg) !important;
    box-shadow: var(--ios-shadow) !important;
    border: var(--ios-border) !important;
    position: relative;
    overflow: hidden;
}

.instagram-media::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(45deg, transparent, rgba(255, 255, 255, 0.3), transparent);
    transform: translateX(-100%);
    transition: transform 0.8s ease;
}

.instagram-media:hover::before {
    transform: translateX(150%);
}

.instagram-media iframe {
    width: 100% !important;
    height: 100% !important;
    min-height: unset !important;
    max-height: none !important;
    border: none !important;
    background: transparent !important;
}

.instagram-media:hover {
    transform: var(--apple-hover-scale) !important;
    box-shadow: var(--apple-hover-shadow) !important;
}

.instagram-media:active {
    transform: var(--apple-active-scale) !important;
    box-shadow: var(--apple-active-shadow) !important;
}

@supports not (backdrop-filter: blur(20px)) {
    .header, .hero, .about, .work, .reviews, .contact, .pricing,
    .magical-border, .price-item, .review, .contact li, nav a,
    .instagram-media {
        background: rgba(255, 240, 245, 0.95);
    }
}

/* Apple-style hover effects */
.header, .hero, .about, .work, .reviews, .contact, .pricing,
.magical-border, .price-item, .review, .contact li, nav a,
.gallery img, .instagram-media {
    transition: var(--transition);
    transform-origin: center;
    will-change: transform, box-shadow;
    backface-visibility: hidden;
}

.header:hover, .hero:hover, .about:hover, .work:hover, 
.reviews:hover, .contact:hover, .pricing:hover,
.magical-border:hover, .price-item:hover, .review:hover, 
.contact li:hover, nav a:hover, .gallery img:hover, 
.instagram-media:hover {
