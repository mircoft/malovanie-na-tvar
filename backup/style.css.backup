:root {
    --primary: #FF69B4;
    --secondary: #FFB6C1;
    --accent: #FF1493;
    --bg-light: #FFF0F5;
    --bg-gradient: linear-gradient(120deg, #FFE4E1 0%, #FFF0F5 100%);
    --text: #2C3E50;
    --text-light: #4A4A4A;
    --shadow: 0 4px 20px rgba(255, 105, 180, 0.15);
    --hover-shadow: 0 8px 32px rgba(255, 105, 180, 0.25);
    --active-shadow: 0 2px 8px rgba(255, 105, 180, 0.1);
    
    --space-xs: 0.5rem;
    --space-sm: 1rem;
    --space-md: 1.5rem;
    --space-lg: 2rem;
    --space-xl: 3rem;
    
    --radius-sm: 12px;
    --radius-md: 16px;
    --radius-lg: 24px;
    --radius-xl: 32px;
    
    --transition: all 0.4s cubic-bezier(0.2, 0, 0, 1);
    --transition-fast: all 0.2s cubic-bezier(0.2, 0, 0, 1);
    
    --fade-in: fadeInUp 0.8s cubic-bezier(0.2, 0, 0, 1) forwards;
    --scale-in: fadeInScale 0.8s cubic-bezier(0.2, 0, 0, 1) forwards;
    
    --hover-scale: scale(1.02);
    --active-scale: scale(0.98);
    
    --blur: blur(20px);
    --glass-bg: rgba(255, 255, 255, 0.7);
    --glass-border: 1px solid rgba(255, 255, 255, 0.18);

    --font: 'Pattaya Regular', cursive, sans-serif;
    --border-radius: 24px;
    --ios-blur: blur(20px);
    --ios-shadow: 0 8px 32px rgba(31, 38, 135, 0.15);
    --ios-border: 1px solid rgba(255, 255, 255, 0.18);
    --ios-transition: all 0.4s cubic-bezier(0.2, 0, 0, 1);
    --ios-hover-transform: translateY(-8px) scale(1.04);
    --ios-active-transform: translateY(-4px) scale(0.96);
    --apple-shadow: 0 4px 12px rgba(0, 0, 0, 0.08);
    --apple-hover-shadow: 0 8px 24px rgba(0, 0, 0, 0.12);
    --apple-active-shadow: 0 2px 8px rgba(0, 0, 0, 0.06);
    --apple-scale: scale(1.02);
    --apple-hover-scale: scale(1.04);
    --apple-active-scale: scale(0.98);

    --body-bg-pink: #FFEBEE;
    --body-bg-gradient-pink: linear-gradient(120deg, #FFCDD2 0%, #FFEBEE 100%);

    /* Apple hero variables */
    --background-color: var(--bg-light);
    --text-color: var(--text);
    --accent-color: var(--primary);
    --apple-ease: cubic-bezier(0.25, 0.1, 0.25, 1);
}

body {
    font-family: 'Poppins', sans-serif;
    color: var(--text);
    line-height: 1.6;
    background: var(--bg-gradient);
    margin: 0;
    padding: 0;
    position: relative;
    min-height: 100vh;
    overflow-x: hidden;
}
body::before {
    content: '';
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: 
        url('data:image/svg+xml;utf8,<svg width="100%25" height="100%25" xmlns="http://www.w3.org/2000/svg"><circle cx="40" cy="40" r="6" fill="%23ffe066"/><circle cx="90" cy="120" r="4" fill="%23b5e0ff"/><circle cx="200" cy="80" r="5" fill="%23ffd6e0"/><circle cx="80" cy="200" r="3" fill="%23e0c3fc"/><circle cx="300" cy="160" r="7" fill="%23ffe066"/></svg>'),
        radial-gradient(circle at 20% 20%, rgba(255, 224, 102, 0.15) 0%, transparent 50%),
        radial-gradient(circle at 80% 40%, rgba(181, 224, 255, 0.15) 0%, transparent 50%),
        radial-gradient(circle at 40% 80%, rgba(255, 214, 224, 0.15) 0%, transparent 50%),
        radial-gradient(circle at 60% 60%, rgba(224, 195, 252, 0.15) 0%, transparent 50%);
    backdrop-filter: var(--ios-blur);
    -webkit-backdrop-filter: var(--ios-blur);
    opacity: 0.8;
    z-index: -1;
}
.header {
    background: rgba(255, 240, 245, 0.7);
    border-radius: 20px;
    margin: 32px auto 48px auto;
    max-width: 1100px;
    padding: 16px 32px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    position: relative;
    z-index: 2;
    flex-wrap: wrap;
    gap: 16px;
}
.logo {
    display: flex;
    align-items: center;
    font-family: 'Pattaya Regular', cursive, sans-serif;
    font-size: 2.8rem;
    font-weight: normal;
    color: var(--primary);
    text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.1);
    letter-spacing: 1px;
    flex-shrink: 0;
}
.logo .dot {
    width: 18px;
    height: 18px;
    background: var(--secondary);
    border-radius: 50%;
    display: inline-block;
    margin-right: 12px;
    box-shadow: 0 0 12px var(--secondary);
}
nav {
    display: flex;
    align-items: center;
    flex-wrap: wrap;
    gap: 10px;
}
nav a {
    margin-left: 0;
    text-decoration: none;
    color: var(--text);
    font-weight: 600;
    font-size: 1.1rem;
    padding: 10px 20px;
    border-radius: 16px;
    transition: var(--transition);
    transform-origin: center;
    will-change: transform, box-shadow, background;
    backface-visibility: hidden;
    background: rgba(255, 255, 255, 0.7);
    backdrop-filter: var(--ios-blur);
    -webkit-backdrop-filter: var(--ios-blur);
    border: var(--ios-border);
    box-shadow: var(--ios-shadow);
}
nav a:hover {
    transform: var(--apple-hover-scale);
    box-shadow: var(--apple-hover-shadow);
    background: rgba(255, 255, 255, 0.9);
}
.hero {
    position: relative;
    min-height: unset;
    padding: 0;
    background: var(--background-color);
    overflow: hidden;
    margin-bottom: var(--space-xl);
}
.hero-content {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 24px;
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: var(--space-xl);
    align-items: center;
    justify-items: center;
}
.hero-text {
    position: relative;
    z-index: 2;
}
.hero-title {
    display: flex;
    flex-direction: column;
    gap: 12px;
    margin-bottom: 16px;
}
.hero-subtitle {
    font-size: 1.5rem;
    font-weight: 500;
    color: var(--accent-color);
    letter-spacing: -0.02em;
}
.hero-main-title {
    font-size: 3.5rem;
    font-weight: 700;
    line-height: 1.1;
    letter-spacing: -0.03em;
    background: linear-gradient(135deg, var(--text-color) 0%, var(--accent-color) 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    opacity: 0;
    transform: translateY(20px);
    animation: var(--fade-in);
}
.hero-description {
    font-size: 1.3rem;
    line-height: 1.5;
    color: var(--text-color);
    margin-bottom: var(--space-lg);
    opacity: 0;
    transform: translateY(20px);
    animation: var(--fade-in);
}
.hero-cta {
    opacity: 0;
    transform: translateY(20px);
    animation: var(--fade-in);
}
.hero-button {
    display: inline-flex;
    align-items: center;
    gap: 12px;
    padding: 16px 32px;
    font-size: 1.125rem;
    font-weight: 500;
    color: var(--background-color);
    background: var(--accent-color);
    border-radius: 30px;
    text-decoration: none;
    transition: all 0.3s var(--apple-ease);
    box-shadow: var(--apple-shadow);
}
.hero-button:hover {
    transform: translateY(-2px) scale(var(--apple-hover-scale));
    box-shadow: var(--apple-hover-shadow);
}
.hero-button:active {
    transform: translateY(0) scale(var(--apple-active-scale));
    box-shadow: var(--apple-active-shadow);
}
.hero-button-arrow {
    width: 20px;
    height: 20px;
    transition: transform 0.3s var(--apple-ease);
}
.hero-button:hover .hero-button-arrow {
    transform: translateX(4px);
}
.hero-image {
    position: relative;
    width: 100%;
    height: auto;
    min-height: unset;
    aspect-ratio: 1 / 1;
    border-radius: 24px;
    overflow: hidden;
    opacity: 0;
    transform: scale(0.95);
    animation: var(--scale-in);
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 0 auto;
}
.hero-image-main {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.8s var(--apple-ease);
}
.hero-image:hover .hero-image-main {
    transform: scale(1.05);
}
.hero-image-overlay {
    position: absolute;
    inset: 0;
    background: linear-gradient(
        to bottom,
        rgba(255, 255, 255, 0) 0%,
        rgba(255, 255, 255, 0.1) 100%
    );
    pointer-events: none;
}
@keyframes fadeInUp {
    from {
        opacity: 0;
        transform: translateY(20px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}
@keyframes fadeInScale {
    from {
        opacity: 0;
        transform: scale(0.95);
    }
    to {
        opacity: 1;
        transform: scale(1);
    }
}
@media (max-width: 1024px) {
    .hero-content {
        grid-template-columns: 1fr;
        text-align: center;
        gap: var(--space-xl);
        align-items: center;
        padding: 0 24px;
    }
    .hero-main-title {
        font-size: 3rem;
    }
    .hero-image {
        max-width: 500px;
        margin: 0 auto;
    }
    .hero-button {
        margin: 0 auto;
    }
}
@media (max-width: 768px) {
    .hero {
        padding: 0;
    }
    .hero-main-title {
        font-size: 2.5rem;
    }
    .hero-description {
        font-size: 1.125rem;
        margin-bottom: 10px;
    }
    .hero-image {
        min-height: unset;
        aspect-ratio: 1 / 1;
    }
}
.magical-sparkles {
    position: absolute;
    top: -20px;
    left: -20px;
    right: -20px;
    bottom: -20px;
    background-image: 
        radial-gradient(circle at 20% 20%, rgba(255, 215, 0, 0.3) 0%, transparent 20%),
        radial-gradient(circle at 80% 40%, rgba(255, 215, 0, 0.3) 0%, transparent 20%),
        radial-gradient(circle at 40% 80%, rgba(255, 215, 0, 0.3) 0%, transparent 20%);
    animation: sparkle 4s ease-in-out infinite;
    pointer-events: none;
}
.magical-title {
    font-family: 'Pattaya Regular', cursive, sans-serif;
    font-size: 4rem;
    font-weight: normal;
    color: var(--text);
    text-align: center;
    margin-bottom: 2rem;
    position: relative;
    line-height: 1.2;
    letter-spacing: 1px;
    text-shadow: 
        3px 3px 0 rgba(255, 182, 193, 0.4),
        -3px -3px 0 rgba(255, 182, 193, 0.4),
        0 0 20px rgba(255, 182, 193, 0.5);
}
.magical-title .word {
    display: inline-block;
    margin: 0 0.15em;
    padding: 0.1em 0.2em;
    background: rgba(255, 255, 255, 0.7);
    border-radius: 10px;
    transform-origin: center;
    animation: wordFloat 3s ease-in-out infinite;
    text-transform: none;
    border: none;
}
.magical-title .word.highlight {
    color: var(--primary);
    background: rgba(255, 105, 180, 0.1);
    text-shadow: 
        3px 3px 0 rgba(255, 182, 193, 0.5),
        -3px -3px 0 rgba(255, 182, 193, 0.5),
        0 0 25px rgba(255, 105, 180, 0.6);
    font-size: 1em;
    border: none;
}
.hero-image img {
    position: relative;
    z-index: 2;
    border-radius: 25px;
    max-width: 700px;
    display: block;
    transition: transform 0.4s ease;
    width: 100%;
    height: auto;
    object-fit: cover;
}
.hero-image img:hover {
    transform: translateY(-5px) scale(1.03);
    box-shadow: 0 16px 40px rgba(255, 105, 180, 0.25);
}
@keyframes sparkle {
    0%, 100% { opacity: 0.3; }
    50% { opacity: 0.6; }
}
@keyframes wordFloat {
    0%, 100% { transform: translateY(0); }
    50% { transform: translateY(-5px); }
}
@keyframes shimmer {
    0% {
        transform: translateX(-150%) rotate(45deg);
    }
    100% {
        transform: translateX(150%) rotate(45deg);
    }
}
@keyframes borderGlow {
    0%, 100% { opacity: 0.3; }
    50% { opacity: 0.6; }
}
@keyframes sparkleRotate {
    from { transform: rotate(0deg); }
    to { transform: rotate(360deg); }
}
@keyframes buttonPulse {
    0% { transform: scale(1); }
    50% { transform: scale(1.1); }
    100% { transform: scale(1); }
}
@media (max-width: 900px) {
    .hero-content {
        flex-direction: column;
        gap: 30px;
    }
    .image-frame {
        padding: 15px;
        border-radius: 30px;
    }
    .image-frame::before {
        top: 7px; left: 7px; right: 7px; bottom: 7px;
        border-radius: 25px;
    }
    .hero-image img {
        max-width: 600px;
    }
    .logo {
        font-size: 2.2rem;
    }
    .magical-title {
        font-size: 3.2rem;
        letter-spacing: 0.5px;
    }
    .magical-btn {
        font-size: 1.1rem;
        padding: 1rem 2rem;
    }
}
@media (max-width: 768px) {
    .hero-image img {
        max-width: 400px;
    }
    .logo {
        font-size: 1.8rem;
    }
    .magical-title {
        font-size: 2.5rem;
        letter-spacing: 0px;
    }
    .about h2, .work h2, .reviews h2, .contact h2, .pricing h2 {
        font-size: 1.8rem;
    }
    .magical-border {
        padding: 1.5rem;
    }
    .magical-btn {
        font-size: 1rem;
        padding: 0.8rem 1.5rem;
        margin-top: 1.5rem;
    }
    .hero {
        padding: 1.2rem 1rem 0 1rem;
    }
    .hero-image img {
        max-width: 100%;
        margin: 0 auto;
    }
    .contact li {
        padding: 12px 16px;
    }
    .contact li svg {
        width: 24px;
        height: 24px;
        padding: 10px;
    }
    .contact li a {
        font-size: 1rem;
    }
}
@media (max-width: 600px) {
    .header {
        flex-direction: column;
        align-items: flex-start;
        padding: 1rem;
    }
    .header nav {
        flex-direction: column;
        align-items: flex-start;
        margin-top: 0;
        gap: 0.4rem;
    }
    nav a {
        margin: 0;
        padding: 8px 16px;
        font-size: 1rem;
    }
    .logo {
        font-size: 1.8rem;
        letter-spacing: 0px;
        flex-shrink: 1;
    }
    .logo .dot {
        width: 14px;
        height: 14px;
        margin-right: 8px;
    }
    .magical-title {
        font-size: 2.5rem;
        letter-spacing: 0px;
    }
    .about h2, .work h2, .reviews h2, .contact h2, .pricing h2 {
        font-size: 1.8rem;
    }
    .magical-border {
        padding: 1.5rem;
    }
    .magical-btn {
        font-size: 1rem;
        padding: 0.8rem 1.5rem;
        margin-top: 1.5rem;
    }
    .hero {
        padding: 1.2rem 1rem 0 1rem;
    }
    .hero-image img {
        max-width: 100%;
        margin: 0 auto;
    }
    .contact li {
        padding: 12px 16px;
    }
    .contact li svg {
        width: 24px;
        height: 24px;
        padding: 10px;
    }
    .contact li a {
        font-size: 1rem;
    }
    .reviews .review {
        padding: 15px;
        width: 100%;
    }
    .reviews .review .stars,
    .reviews .review p,
    .reviews .review .author {
        padding: 0;
    }
    .reviews .review {
        padding-top: 15px;
        padding-bottom: 15px;
    }
}
main {
    padding: 2rem 32px;
    max-width: 1100px;
    margin: 0 auto;
    box-sizing: border-box;
}
.about,
.work,
.reviews,
.contact,
.pricing {
    background: var(--glass-bg);
    backdrop-filter: var(--blur);
    -webkit-backdrop-filter: var(--blur);
    border: var(--glass-border);
    border-radius: var(--radius-xl);
    padding: var(--space-xl);
    margin-bottom: var(--space-xl);
    position: relative;
    overflow: hidden;
    z-index: 2;
    display: block;
    box-sizing: border-box;
    width: 100%;
    max-width: 1100px;
    margin-left: auto;
    margin-right: auto;
    opacity: 0;
    animation: var(--fade-in);
}
.about::before,
.work::before,
.reviews::before,
.contact::before,
.pricing::before {
    content: '';
    position: absolute;
    inset: 0;
    background: linear-gradient(
        to bottom,
        rgba(255, 255, 255, 0) 0%,
        rgba(255, 255, 255, 0.1) 100%
    );
    pointer-events: none;
}
.about h2,
.work h2,
.reviews h2,
.contact h2,
.pricing h2,
.gallery-title {
    font-family: 'Poppins', sans-serif;
    font-size: 2.5rem;
    font-weight: 700;
    line-height: 1.2;
    letter-spacing: -0.03em;
    background: linear-gradient(135deg, var(--text) 0%, var(--accent) 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    margin-bottom: var(--space-lg);
    position: relative;
    display: inline-block;
    opacity: 0;
    animation: var(--fade-in);
}
.about h2::after,
.work h2::after,
.reviews h2::after,
.contact h2::after,
.pricing h2::after {
    display: none;
}
.about {
    display: flex;
    flex-direction: column;
    gap: 2rem;
}

.about-content {
    display: flex;
    flex-direction: column;
    gap: 2rem;
    max-width: 800px;
    margin: 0 auto;
}

.about-text {
    display: flex;
    flex-direction: column;
    gap: 1.5rem;
}

.about-text p {
    font-family: 'Poppins', sans-serif;
    font-size: 1.25rem;
    line-height: 1.8;
    color: var(--text-light);
    margin: 0;
    opacity: 0;
    animation: var(--fade-in);
    text-align: justify;
    hyphens: auto;
}

.about-text p:first-of-type {
    animation-delay: 0.3s;
}

.about-text p:last-of-type {
    animation-delay: 0.4s;
    font-weight: 500;
    color: var(--accent-color);
    text-align: center;
    font-size: 1.3rem;
    padding: 1rem;
    background: rgba(255, 255, 255, 0.5);
    border-radius: 16px;
    box-shadow: var(--apple-shadow);
}

.o-mne-img {
    width: 240px;
    height: 240px;
    object-fit: cover;
    object-position: 55% 40%;
    border-radius: 24px;
    box-shadow: var(--apple-shadow);
    margin: 0 auto 2rem auto;
    background: var(--background-color);
    border: none;
    overflow: hidden;
    opacity: 0;
    transform: translateY(20px);
    animation: var(--fade-in);
    transition: var(--transition);
}

.o-mne-img:hover {
    transform: scale(1.02);
    box-shadow: var(--apple-hover-shadow);
}

@media (min-width: 768px) {
    .about-content {
        flex-direction: row;
        align-items: flex-start;
        gap: 3rem;
    }

    .o-mne-img {
        margin: 0;
        flex-shrink: 0;
    }

    .about-text {
        flex: 1;
    }

    .about-text p {
        font-size: 1.3rem;
    }

    .about-text p:last-of-type {
        font-size: 1.4rem;
    }
}

@media (max-width: 767px) {
    .about-content {
        flex-direction: column;
        align-items: center;
        text-align: center;
    }

    .about-text p {
        text-align: center;
    }

    .o-mne-img {
        width: 200px;
        height: 200px;
    }
}

@media (max-width: 480px) {
    .about-text p {
        font-size: 1.1rem;
        line-height: 1.6;
    }

    .about-text p:last-of-type {
        font-size: 1.2rem;
        padding: 0.8rem;
    }

    .o-mne-img {
        width: 180px;
        height: 180px;
    }
}
.gallery {
    display: flex;
    flex-wrap: wrap;
    gap: var(--space-lg);
    justify-content: center;
    margin: 0 auto var(--space-xl) auto;
    max-width: 1000px;
    padding: 0 20px;
    position: relative;
}
.gallery::before {
    content: '';
    position: absolute;
    top: -20px;
    left: -20px;
    right: -20px;
    bottom: -20px;
    background: radial-gradient(circle at 50% 50%, 
        rgba(255, 105, 180, 0.1) 0%,
        rgba(255, 182, 193, 0.05) 50%,
        transparent 100%);
    z-index: -1;
    animation: pulseGlow 4s ease-in-out infinite;
}
.gallery img {
    width: calc(50% - 10px);
    max-width: 450px;
    border-radius: var(--radius-lg);
    box-shadow: 0 10px 40px rgba(31, 38, 135, 0.2);
    border: var(--ios-border);
    display: block;
    opacity: 0;
    transform: scale(0.92) translateY(30px);
    transition: var(--transition);
    position: relative;
    overflow: hidden;
}
.gallery img::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(45deg, transparent, rgba(255, 255, 255, 0.8), transparent);
    transform: translateX(-100%);
    transition: transform 5s ease;
}
.gallery img::after {
    content: '';
    position: absolute;
    top: -50%;
    left: -50%;
    width: 200%;
    height: 200%;
    background: linear-gradient(
        45deg,
        transparent 0%,
        rgba(255, 255, 255, 0.1) 45%,
        rgba(255, 255, 255, 0.2) 50%,
        rgba(255, 255, 255, 0.1) 55%,
        transparent 100%
    );
    transform: rotate(45deg);
    animation: shimmer 3s infinite;
    z-index: 2;
    opacity: 0;
}
.gallery img.visible {
    opacity: 1;
    transform: scale(1) translateY(0);
    animation: imagePop 0.7s cubic-bezier(.34,1.56,.64,1) forwards;
}
.gallery img:hover {
    transform: var(--apple-hover-scale);
